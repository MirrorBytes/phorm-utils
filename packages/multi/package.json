  "license": "ISC",
  "main": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "author": {
    "name": "MirrorBytes <smokebytes@gmail.com>"
  },
  "homepage": "https://github.com/MirrorBytes/phorm-utils/tree/main/packages/multi",
