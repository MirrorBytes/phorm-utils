    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-svelte3": "^3.0.0",
    "eslint-svelte3-preprocess": "^0.0.4",
    "jest": "^26.6.3",
    "jest-axe": "^4.1.0",
    "prettier": "^2.2.1",
